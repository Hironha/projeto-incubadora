{
	"name": "incubadora-backend",
	"version": "1.0.0",
	"description": "incubadora backend para disciplina de projeto de sistemas",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"start": "node dist/server.js",
		"build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
		"dev": "cross-env NODE_ENV=dev ts-node-dev -r tsconfig-paths/register --respawn --ignore-watch node_modules --no-notify src/server.ts"
	},
	"devDependencies": {
		"@babel/cli": "^7.18.9",
		"@babel/core": "^7.18.9",
		"@babel/node": "^7.18.9",
		"@babel/plugin-proposal-decorators": "^7.18.10",
		"@babel/preset-env": "^7.18.9",
		"@babel/preset-typescript": "^7.18.6",
		"@types/express": "^4.17.13",
		"@types/express-ws": "^3.0.1",
		"@types/ws": "^8.5.3",
		"babel-plugin-module-resolver": "^4.1.0",
		"cross-env": "^7.0.3",
		"ts-node-dev": "^2.0.0",
		"tsconfig-paths": "^4.0.0",
		"typescript": "^4.7.4"
	},
	"dependencies": {
		"class-validator": "^0.13.2",
		"dotenv": "^16.0.1",
		"express": "^4.18.1",
		"express-ws": "^5.0.2",
		"firebase-admin": "^11.0.1",
		"helmet": "^5.1.1"
	}
}
